<div class="contenedor">
  <div class="divTitulo">
    <span class="titulo">
      Archivos fotográficos
    </span>
  </div>

  <%= link_to 'Nuevo archivo fotográfico', new_archivofotografico_path, class:"btn btnNoActivo" %>
  <div class="row">
    <% @archivofotograficos.each do |archivofotografico| %>
      <div class="col-md-3">  <%= archivofotografico.titulo %></div>
      <div class="col-md-3">  <%= archivofotografico.descripcion %></div>
      <div class="col-md-2">  <%= archivofotografico.anio %></div>
      <div class="col-md-2">  <%= archivofotografico.encargado %></div>
      <div class="col-md-2">  <%= link_to 'Edit', edit_archivofotografico_path(archivofotografico) %>
        <%= link_to 'Destroy', archivofotografico, method: :delete, data: { confirm: 'Esta seguro?' } %></div>

    <% end %>
  </div>

</div>
