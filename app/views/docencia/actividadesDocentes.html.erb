      </div><br>
      <div class="derecha">
          <%= image_tag "docencia/trabajo_social.png"%>
      </div><br>
      <div class="derecha">
        <%= image_tag "docencia/logoUniversita.png" %>
      </div><br>
      <div class="derecha">
        <%= image_tag "docencia/gemma.png" %>
      </div><br>
      <div class="derecha">
