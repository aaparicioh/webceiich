
<div class="contenedor">
  <div class="divTitulo">
    <span class="titulo">
      Adscripciones
    </span>
  </div>
  <p id="notice"><%= notice %></p>
  <div class="row">
    <% @adscripciones.each do |adscripcion| %>
    <div class="col-md-2">

    </div>
    <div class="col-md-4">
      <%= adscripcion.nombre %>

    </div>
    <div class="col-md-4">
      <%= link_to 'Editar', edit_adscripcion_path(adscripcion),class: "link" %>
      <%= link_to 'Destruir', adscripcion, method: :delete, data: { confirm: 'Estas seguro?' },class: "link" %>

    </div>
    <div class="col-md-2">

    </div>

    <% end %>
  </div>
